qm_mode=0
extended_inetd=1
tcpd_path=/usr/sbin/tcpd
sort_mode=0
restart_command=killall -HUP inetd
allow_file=/etc/hosts.allow
protocols_file=/etc/protocols
rpc_inetd=1
