S1=$(sudo blkid | sed -n -e "s/\/dev\/sda1:\s\(.*\).*/\1/p" | sed -n -e "s/UUID=\(.*\)\sB.*/\1/p" | sed 's/\"/\\"/g')

filename='fdisk.txt'
n=1
while read line; do
# reading each line
shd=$(sudo $line | sed 's/\//\\\//g')
S1=$(sudo blkid | sed -n -e "s/$shd:\s\(.*\).*/\1/p" | sed -n -e "s/UUID=\(.*\)\sB.*/\1/p" | sed 's/\"/\\"/g')
sed -i -e "$ a UUID\=$S1	\/opt\/	ext4	defaults	0	2" /etc/fstab

n=$((n+1))
done < $filename